{
  "schemaVersion": 1,
  "id": "silveroak",
  "version": "${version}",
  "name": "Silveroak Outpost",
  "description": "This is a simple library that provides environment framework like temperature, rainfall, and humidity, also other development tools.",
  "authors": [
    "RoshioLeo",
    "cnlimiter"
  ],
  "contact": {
    "homepage": "https://github.com/RoShioLeo",
    "sources": "https://github.com/lemon-slice-studio/Silveroak-Outpost"
  },
  "license": "BSD-3",
  "icon": "assets/silveroak/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "cloud.lemonslice.silveroak.data.DataGenerationHandler"
    ],
    "main": [
      "cloud.lemonslice.silveroak.SilveroakOutpost"
    ],
    "client": [
      "cloud.lemonslice.silveroak.client.gui.hud.OverlayHandler"
    ]
  },
  "accessWidener": "silveroak.accesswidener",
  "mixins": [
    "silveroak.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
